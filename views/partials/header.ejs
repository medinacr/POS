<script>
  (() => {
      function initializeClock() {
      const clockTime = document.querySelector('.clock-time')
      const clockDate = document.querySelector('.clock-date')

      const date = new Date();
      const monthNames = ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"];
      const dateString = monthNames[date.getMonth()] + " "+ date.getDate() + " " + date.getFullYear();

      clockDate.innerHTML = dateString

      console.log(dateString);
      setInterval(() => {
        clockTime.innerHTML = new Date().toLocaleTimeString()
      }, 1000);
      
    }

    function setActive() {
      const path = window.location.pathname
      const menu = document.getElementsByClassName(path)[0]
      menu.className += ' nav--item_active'
    }

    setActive()
    initializeClock()
  })()
  
</script>